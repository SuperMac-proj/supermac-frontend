# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Paddle Configuration
# Environment: 'sandbox' for testing, 'production' for live
VITE_PADDLE_ENVIRONMENT=sandbox

# Get this from Paddle Dashboard > Developer Tools > Authentication
VITE_PADDLE_CLIENT_TOKEN=your_paddle_client_token_here

# Paddle Price IDs (from Paddle Dashboard > Catalog > Prices)
# Create these price IDs in your Paddle dashboard first
VITE_PADDLE_PRICE_ID_MONTHLY=pri_monthly_id_here
VITE_PADDLE_PRICE_ID_YEARLY=pri_yearly_id_here
VITE_PADDLE_PRICE_ID_TRIAL=pri_trial_id_here

# Backend API URL (will be needed for subscription management)
# This should point to your backend API that handles Paddle webhooks and subscription management
VITE_API_BASE_URL=http://localhost:3000/api
